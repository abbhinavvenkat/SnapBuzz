#feedback:focus {
        	border: 2px solid #09C;
        	outline: 0 none;
        	}

#feedback{
	    width:40em;
	    display:block;
		float:middle;
	    border: 1px solid #999;
		position:absolute;
		margin-top:0em;
		height: 5em;
        -webkit-box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.3);
	    -moz-box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.3);
	    box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.3);
		margin-left:-2.7em;	
		-webkit-box-sizing: border-box;
		-moz-box-sizing: border-box;
        box-sizing: border-box;
		overflow:hidden;
		padding:0px 0.9em;
	    }
#comment-block{
		margin-left:11.5%;
		height:12em;
		}

#foot-image{
		position:relative;
        bottom:0;
        width:100%;
        height:5em;
		background:-webkit-gradient(linear, bottom, left 175px, from(#0A0A1F), to(#222235));
        background: -moz-linear-gradient(bottom, #0A0A1F, #222235 175px); 
		/*background:-webkit-gradient(linear, bottom, left 175px, from(#0A0A1F), to(#222235));
        background: -moz-linear-gradient(bottom, #0A0A1F, #222235 175px); */
		/*-webkit-box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.9);
	    -moz-box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.9);
	    box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.9); */
		background:url("../../static/images/blockimg.jpg");
		margin-top:-5em;
		clear:both; 
	}
#img-size{
	 max-width: 90%;
    max-height: 90%;
    margin: 2%;
	}
#photo-img{
	margin-top:1em;
	position:relative;
	}

#navg{
	margin-top:4em;
	color:#3399FF;
	}
#back{
	margin-left:20%;
	color:#3399FF;
	}
#prev{
	margin-left:39%;
	color:#3399FF;
	}
#next{
	margin-left:0%;
	color:#3399FF;
	}
#navg a:hover{
	color:#9BEBFF;
	}
#details-block{
	margin-left:20%;
	margin-top:0.8em;
	}
#details-block a:hover{
	color:#9BEBFF;
	}
#like,#share,#Tag{
	color:#3399FF;
	}
#like,#Tag{
	position:absolute;
	margin-top:0.6em;
	}
#like{
	margin-left:2.5em;
	}
#Tag{
	margin-left:5em;
	}
#like,#share,#Tag,#next,#prev,#back{
	font-weight:bold;
	}
#comment-block{
	position:relative;
	}
#postcomment{
	cursor:pointer;
	position:absolute;
	width:14em;
	margin-top:-7.5em;
	margin-left:3.4em;
    background:#3399FF;
    color:#fff;
    font-family: Tahoma, Geneva, sans-serif;
    height:3em;
    border: 1px solid #3399FF;
	font-size:12px;
    font-weight: bold;
	-webkit-box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.3);
	-moz-box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.3);
	box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.3);
    }
