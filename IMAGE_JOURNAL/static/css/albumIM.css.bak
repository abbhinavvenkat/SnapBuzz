	.codrops-header {
	position: relative;
	margin: 0 auto;
	padding: 1.5em 1em;
	color: #fff;
	text-align: center;
	background: url("../images/imgalbum.jpg") no-repeat center center;
	background-size: cover;
	}

	.codrops-header h1 {
	font-size: 2.625em;
	line-height: 1.3;
	margin: 0;
	font-weight: 400;
	
	}
        .main {
                /*padding:10px;*/
                padding-bottom:5em;  
        }
        .foot {
                position:relative;
                bottom:0;
                width:100%;
                height:5em;
		background:-webkit-gradient(linear, bottom, left 175px, from(#0A0A1F), to(#222235));
                background: -moz-linear-gradient(bottom, #0A0A1F, #222235 175px); 
		/*background:-webkit-gradient(linear, bottom, left 175px, from(#0A0A1F), to(#222235));
                background: -moz-linear-gradient(bottom, #0A0A1F, #222235 175px); */
