#formstyle{
		position:relative;
		height:6em;
		background:url("../images/blockimg.jpg");
		width:100%;
		-webkit-box-shadow: 0px 0px 20px rgba(0, 0, 0, 0.3);
	    -moz-box-shadow: 0px 0px 20px rgba(0, 0, 0, 0.3);
	    box-shadow: 0px 0px 20px rgba(0, 0, 0, 0.3);
}

#searchbutton {
	    cursor:pointer;
	    width:6em;
        position:absolute;
        margin-top:2.35em;
        left:1em;
        background:#3399FF;
        color:#fff;
        font-family: Tahoma, Geneva, sans-serif;
        height:3.38em;
        border: 1px solid #3399ff;
		font-size:12px;
        font-weight: bold;
		-webkit-box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.3);
	    -moz-box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.3);
	    box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.3);
		margin-left:3em;
}
		
#searchover input:hover {
        	background:#99E6FF;
        	color:#3399FF;
        	font-weight: bold;	
        	-webkit-transition: 200ms linear 0s;
        	-moz-transition: 200ms linear 0s;
        	-o-transition: 200ms linear 0s;
        	transition: 200ms linear 0s;
        	outline: 0 none;
			border: 1px solid #99E6FF;
}
		
#createbutton {
	    cursor:pointer;
	    width:15em;
        position:absolute;
        margin-top:2.35em;
        left:42em;
        background:#990033;
        color:#fff;
        font-family: Tahoma, Geneva, sans-serif;
        height:3.38em;
        border: 1px solid #990033;
		font-size:12px;
        font-weight: bold;
		-webkit-box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.3);
	    -moz-box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.3);
	    box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.3);
}
		
#createhover input:hover {
        background:#CC8099;
        color:#990033;
        font-weight: bold;	
        -webkit-transition: 200ms linear 0s;
        -moz-transition: 200ms linear 0s;
        -o-transition: 200ms linear 0s;
        transition: 200ms linear 0s;
        outline: 0 none;
		border: 1px solid #CC8099;
}

#searchinput{
	    width:22em;
	    display:block;
		float:middle;
	    border: 1px solid #999;
		margin-top:1.8em;
	    height: 2.5em;
	    -webkit-box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.3);
	    -moz-box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.3);
	    box-shadow: 0px 0px 8px rgba(0, 0, 0, 0.3);
		margin-left:7.4em;	
		position:absolute;
		padding:0px 0.5em;
}
		
